        "agentDir": "/home/node/.openclaw/agents/work/agent",
        "model": "moonshot/kimi-k2.5",
        "sandbox": {
          "mode": "off"
        },
        "tools": {
          "allow": [
            "read",
            "web_search",
            "tavily_search",
            "sessions_list",
            "sessions_history",
            "sessions_send",
            "sessions_spawn",
            "session_status"
