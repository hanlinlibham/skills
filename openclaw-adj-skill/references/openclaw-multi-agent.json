        "identity": {
          "name": "Research",
          "emoji": "üî¨"
        },
        "workspace": "/home/node/.openclaw/workspace/research",
        "agentDir": "/home/node/.openclaw/agents/research/agent",
        "model": "moonshot/kimi-k2-thinking",
        "sandbox": {
          "mode": "off"
        },
        "tools": {
          "allow": [
            "read",
            "web_search",
            "tavily_search",
            "browser",
            "sessions_list",
            "sessions_history"
          ],
          "deny": [
            "write",
            "edit",
            "apply_patch",
            "exec",
            "bash"
          ]
