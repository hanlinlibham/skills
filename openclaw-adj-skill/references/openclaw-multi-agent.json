        "workspace": "/home/node/.openclaw/workspace/coding",
        "agentDir": "/home/node/.openclaw/agents/coding/agent",
        "model": "moonshot/kimi-k2.5",
        "sandbox": {
          "mode": "off"
        },
        "tools": {
          "allow": [
            "read",
            "write",
            "edit",
            "apply_patch",
            "exec",
            "bash",
            "web_search",
            "sessions_list",
            "sessions_history"
          ],
          "deny": [
            "canvas",
            "nodes",
            "cron"
          ]
        },
        "groupChat": {
          "mentionPatterns": ["@code", "@coding", "@ÁºñÁ®ã", "@‰ª£Á†Å", "@dev"]
