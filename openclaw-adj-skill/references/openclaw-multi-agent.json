    "defaults": {
      "model": { "primary": "moonshot/kimi-k2.5" },
      "maxConcurrent": 4,
      "subagents": { "maxConcurrent": 8 }
    }
  },
  "tools": {
    "agentToAgent": {
      "enabled": true,
      "allow": ["work", "research", "coding"]
    }
  },
  "bindings": [
    {
      "agentId": "main",
      "match": { "channel": "telegram" }
    }
  ],
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "open",
      "botToken": "your-bot-token",
      "allowFrom": ["*"],
      "groupPolicy": "allowlist",
      "streamMode": "partial"
    }
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "lan",
    "trustedProxies": [
      "172.18.0.1",
      "172.18.0.0/16",
      "127.0.0.1"
    ],
    "auth": {
      "mode": "token",
      "token": "generate-a-secure-random-token"
    }
  }
}
